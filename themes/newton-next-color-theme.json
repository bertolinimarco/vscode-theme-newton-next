      "settings": {
        "foreground": "#aaaaaa"
      }
    },
    {
      "name": "JS - Constant - all",
      "scope": "constant.language.import-export-all.js",
      "settings": {
        "foreground": "#c9c"
      }
    },
    // {
    //   "name": "JS - Variable > Parameters",
    //   "scope": "variable.parameter.js",
    //   "settings": {
